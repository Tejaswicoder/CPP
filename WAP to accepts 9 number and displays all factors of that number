#include <iostream>

int main() {
    int number;

    // Ask the user to enter a number
    std::cout << "Enter a number: ";
    std::cin >> number;

    // Display the factors of the number
    std::cout << "Factors of " << number << " are: ";
    for (int i = 1; i <= number; ++i) {
        if (number % i == 0) {
            std::cout << i << " ";
        }
    }

    std::cout << std::endl;
    return 0;
}
